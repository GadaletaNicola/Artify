<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#A1CF68" />
            <gradient android:angle="-90" android:startColor="#A1CF68" android:endColor="#aec81d"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#c2de99" />
            <solid android:color="#A1CF68"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#A1CF68" />
            <gradient android:angle="-90" android:startColor="#A1CF68" android:endColor="#e7f2a8" />
        </shape>
    </item>
</selector>